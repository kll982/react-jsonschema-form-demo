{
  "include": ["./src/**/*", "custom.d.ts"],
  "exclude": [],
  "compilerOptions": {
    "outDir": "./dist/",
    "jsx": "react",
    "allowJs": true,
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true /* Skip type checking all .d.ts files. */
  }
}
